{% extends "base.html.twig" %}

{% block title %}Posts{% endblock %}

{% block body %}


<h1>Welcome to the Prueba:index page</h1>

{% for post in posts %}
<div class="card">
    <div class="card-header">
      {{ post.title }}
    </div>
    <div class="card-body">
      <blockquote class="blockquote mb-0">
        <p>{{ post.body}} </p>
        <footer class="blockquote-footer">{{ post.createAt|date("m/d/Y H:i:s")}}</footer>
      </blockquote>
    </div>
  </div>
  {% endfor %}



{% endblock %}
